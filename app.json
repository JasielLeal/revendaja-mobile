{
  "expo": {
    "name": "Revendaja",
    "slug": "revendaja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF7100"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bg.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "package": "com.szreactioon.revendaja",
      "permissions": ["NOTIFICATIONS", "CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f72c3ea6-e4d0-47b8-9ce5-ba720c38e40c"
      }
    }
  }
}
